{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist/vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/vite/$1"
    }
  ],
  "functions": {
    "dist/server/index.js": {
      "runtime": "nodejs24.x",
      "memory": 3008,
      "maxDuration": 300
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}